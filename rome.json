{
	"$schema": "./node_modules/rome/configuration_schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "space",
		"indentSize": 2
	},
	"files": {
		"ignore": [
			"public",
			"dist",
			"router.ts",
			"__service__"
		]
	}
}